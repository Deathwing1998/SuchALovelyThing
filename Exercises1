#include <iostream>

using namespace std;



void func(string str, int (&count)[6]) {

	for (int i = 0; i < 6; i++) {

		count[i] = 0;

	}

	for (int i = 0; i < str.length(); i++) {

		if (str[i] == 'a') {

			count[0]++;

		} else if (str[i] == 'e')  {

			count[1]++;

		} else if (str[i] == 'o')  {

			count[2]++;

		} else if (str[i] == 'i')  {

			count[3]++;

		} else if (str[i] == 'u')  {

			count[4]++;

		} else if (str[i] == 'y')  {

			count[5]++;

		}

	}

}



int main() {

	string str;

	int k = 0, t[6];

	char s[] = {'a', 'e', 'o', 'i', 'u', 'y'};

	getline(cin, str);

	func(str, t);

	for (int i = 0; i < (sizeof(t)/sizeof(t[0])); i++) {

		k += t[i];

		cout<<s[i]<<" - "<<t[i]<<endl;

	}

	cout<<"all = "<<k<<endl;

	return 0;

}
